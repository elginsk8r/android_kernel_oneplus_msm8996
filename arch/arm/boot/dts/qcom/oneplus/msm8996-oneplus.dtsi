		reg = <0x28>;
		nxp,pn544-irq = <&tlmm 9 0x00>;
		nxp,pn544-ven = <&tlmm 94 0x00>;
		nxp,pn544-fw-dwnld = <&tlmm 12 0x00>;
		nxp,pn544-ese-pwr = <&pm8994_gpios 07 0x00>;
		nxp,pn544-clk-gpio = <&pm8994_gpios 10 0x00>;
		interrupt-parent = <&tlmm>;
		qcom,clk-src = "BBCLK2";
		interrupts = <9 0>;
		interrupt-names = "nfc_irq";
		clocks = <&clock_gcc clk_bb_clk2_pin>;
		clock-names = "ref_clk";
		nfc_voltage_s4-supply = <&pm8994_s4>;
	};
};

&spi_0 { /* BLSP1 QUP1 */
	p61-nxp-nfc@1 { //Slave driver and CS ID
		compatible = "nxp,p61"; //Manufacture, and Model
		reg = <1>; //Same as CS ID
		nxp,p61-irq = <&tlmm 34 0x00>; //Pass a GPIO
		nxp,p61-rst = <&tlmm 21 0x00>; //Pass a GPIO
		spi-max-frequency = <19200000>;
		spi-cs-high; //CS Active High
		interrupt-parent = <&tlmm>; //GPIO Handler
		interrupts = <34 0>; //GPIO # and flags
	};
};

