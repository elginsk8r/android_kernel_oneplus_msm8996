#include "dsi-panel-samsung-s6e3fa3-1080p-cmd.dtsi"
#include "dsi-panel-samsung-s6e3fa5-1080p-cmd.dtsi"

/ {
	aliases {
		i2c3 = &i2c_3;
	};

	reserved-memory {
		ramoops_mem: ramoops_region@ac000000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0xAC000000 0 0x200000>;
		};
	};
};

&firmware {
	android {
		fstab {
			/delete-node/ vendor;
			system {
				compatible = "android,system";
				dev = "/dev/block/platform/soc/624000.ufshc/by-name/system";
				type = "ext4";
				mnt_flags = "ro,barrier=1,discard";
				fsmgr_flags = "wait,verify";
				status = "ok";
			};
		};
	};
};

