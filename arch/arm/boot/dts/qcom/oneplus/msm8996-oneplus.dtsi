};

&i2c_6 {
	qcom,clk-freq-out = <100000>;
	qcom,fs-clk-div =<104>;
	qcom,high-time-clk-div=<82>;
	bq27541-battery@55 {
		status = "ok";
		compatible = "ti,bq27541-battery";
		reg = <0x55>;
	};
	oneplus_fastcg@26{
		status = "ok";
		compatible = "microchip,oneplus_fastcg";
		reg = <0x26>;
		microchip,mcu-en-gpio = <&tlmm 119 0x00>;
		microchip,usb-sw-gpio = <&tlmm 135 0x00>;
		microchip,ap-clk = <&tlmm 75 0x00>;
		microchip,ap-data = <&tlmm 74 0x00>;
		pinctrl-names = "mux_fastchg_active",
				"mux_fastchg_suspend",
				"mcu_data_active",
				"mcu_data_suspend";
		pinctrl-0 = <&fastchg_active
		&usb_sw_active
		&ap_clk_active >;
		pinctrl-1 = <&usb_sw_suspend
		&fastchg_suspend
		&ap_clk_suspend>;
		pinctrl-2 =<&ap_data_active>;
		pinctrl-3 =<&ap_data_suspend>;
		clocks = <&clock_gcc clk_uart_snoc_msmbus_a_clk>,
			 <&clock_gcc clk_uart_cnoc_msmbus_a_clk>;
		clock-names = "snoc", "cnoc";

	};
};

&pmi8994_fg {
	qcom,battery-data = <&op_batterydata>;
	/delete-property/ qcom,ext-sense-type;
	qcom,resume-soc = <99>;
	qcom,fg-iterm-ma = <200>;
	qcom,fg-chg-iterm-ma = <165>;
	qcom,fg-cc-cv-threshold-mv = <4310>;
	qcom,fg-cutoff-voltage-mv=<3200>;
	oem,use_external_fg;
};

&usb_otg_switch {
	status = "disabled";
};

&pm8994_mpps {
	mpp@a100 { /* MPP 2 */
		qcom,mode = <4>; /* AIN input */
		qcom,ain-route = <1>; /* AMUX 6 ,QPNP_PIN_AIN_AMUX_CH6   = 1*/
		qcom,master-en = <1>; /* Enable MPP */
		qcom,src-sel = <0>; /* Function constant */
		/delete-property/ status;
	};

	mpp@a300 { /* MPP 4 */
		qcom,mode = <4>; /* AIN input */
		qcom,invert = <1>; /* Enable MPP */
		qcom,ain-route = <3>; /* AMUX 8 */
		qcom,master-en = <1>;
		qcom,src-sel = <0>; /* Function constant */
	};

	mpp@a400 { /* MPP 5 */
		qcom,mode = <4>; /* AIN input */
		qcom,ain-route = <0>; /* AMUX 5 ,QPNP_PIN_AIN_AMUX_CH5   = 0*/
		qcom,master-en = <1>; /* Enable MPP */
		qcom,src-sel = <0>; /* Function constant */
		/delete-property/ status;
	};

	mpp@a600 { /* MPP 7 */
		qcom,mode = <4>; /* AIN input */
		qcom,ain-route = <2>; /* AMUX 7 ,QPNP_PIN_AIN_AMUX_CH7   = 2*/
		qcom,master-en = <1>; /* Enable MPP */
		qcom,src-sel = <0>; /* Function constant */
		/delete-property/ status;
	};

	mpp@a700 { /* MPP 8 */
		qcom,mode = <4>; /* AIN input */
		qcom,ain-route = <3>; /* AMUX 6 ,QPNP_PIN_AIN_AMUX_CH8   = 3*/
		qcom,master-en = <1>; /* Enable MPP */
		qcom,src-sel = <0>; /* Function constant */
		/delete-property/ status;
	};
};

&pm8994_rtc {
	qcom,qpnp-rtc-alarm-pwrup = <1>;
};

&pmi8994_gpios {
	gpio@c400 {//pmi8996 gpio 5 type-c IC INT pin
		qcom,mode = <0>;
		qcom,pull = <5>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
		status = "okay";

	};

	gpio@c500 {//pmi8996 gpio 6 type-c IC OTG_USB_ID pin
		qcom,mode = <0>;
		qcom,pull = <5>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
		status = "okay";
	};
};

